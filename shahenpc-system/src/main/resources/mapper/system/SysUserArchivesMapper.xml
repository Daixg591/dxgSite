<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shahenpc.system.mapper.SysUserArchivesMapper">
    
    <resultMap type="SysUserArchives" id="SysUserArchivesResult">
        <result property="archivesId"    column="archives_id"    />
        <result property="userId"    column="user_id"    />
        <result property="train"    column="train"    />
        <result property="attend"    column="attend"    />
        <result property="consideration"    column="consideration"    />
        <result property="proposal"    column="proposal"    />
        <result property="inspection"    column="inspection"    />
        <result property="visit"    column="visit"    />
        <result property="find"    column="find"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectSysUserArchivesVo">
        select archives_id, user_id, train, attend, consideration, proposal, inspection, visit, find, create_by, create_time, update_by, update_time, remark from sys_user_archives
    </sql>

    <select id="selectSysUserArchivesList" parameterType="SysUserArchives" resultMap="SysUserArchivesResult">
        <include refid="selectSysUserArchivesVo"/>
        <where>  
            <if test="userId != null "> and user_id = #{userId}</if>
            <if test="train != null  and train != ''"> and train = #{train}</if>
            <if test="attend != null  and attend != ''"> and attend = #{attend}</if>
            <if test="consideration != null  and consideration != ''"> and consideration = #{consideration}</if>
            <if test="proposal != null  and proposal != ''"> and proposal = #{proposal}</if>
            <if test="inspection != null  and inspection != ''"> and inspection = #{inspection}</if>
            <if test="visit != null  and visit != ''"> and visit = #{visit}</if>
            <if test="find != null  and find != ''"> and find = #{find}</if>
        </where>
    </select>
    
    <select id="selectSysUserArchivesByArchivesId" parameterType="Long" resultMap="SysUserArchivesResult">
        <include refid="selectSysUserArchivesVo"/>
        where archives_id = #{archivesId}
    </select>
        
    <insert id="insertSysUserArchives" parameterType="SysUserArchives" useGeneratedKeys="true" keyProperty="archivesId">
        insert into sys_user_archives
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="userId != null">user_id,</if>
            <if test="train != null">train,</if>
            <if test="attend != null">attend,</if>
            <if test="consideration != null">consideration,</if>
            <if test="proposal != null">proposal,</if>
            <if test="inspection != null">inspection,</if>
            <if test="visit != null">visit,</if>
            <if test="find != null">find,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="userId != null">#{userId},</if>
            <if test="train != null">#{train},</if>
            <if test="attend != null">#{attend},</if>
            <if test="consideration != null">#{consideration},</if>
            <if test="proposal != null">#{proposal},</if>
            <if test="inspection != null">#{inspection},</if>
            <if test="visit != null">#{visit},</if>
            <if test="find != null">#{find},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateSysUserArchives" parameterType="SysUserArchives">
        update sys_user_archives
        <trim prefix="SET" suffixOverrides=",">
            <if test="userId != null">user_id = #{userId},</if>
            <if test="train != null">train = #{train},</if>
            <if test="attend != null">attend = #{attend},</if>
            <if test="consideration != null">consideration = #{consideration},</if>
            <if test="proposal != null">proposal = #{proposal},</if>
            <if test="inspection != null">inspection = #{inspection},</if>
            <if test="visit != null">visit = #{visit},</if>
            <if test="find != null">find = #{find},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where archives_id = #{archivesId}
    </update>

    <delete id="deleteSysUserArchivesByArchivesId" parameterType="Long">
        delete from sys_user_archives where archives_id = #{archivesId}
    </delete>

    <delete id="deleteSysUserArchivesByArchivesIds" parameterType="String">
        delete from sys_user_archives where archives_id in 
        <foreach item="archivesId" collection="array" open="(" separator="," close=")">
            #{archivesId}
        </foreach>
    </delete>

    <!--============================================================自定义-->
    <select id="selectSysUserArchivesByUserId" resultMap="SysUserArchivesResult" parameterType="Long">
        <include refid="selectSysUserArchivesVo"/>
        where user_id = #{userId}
    </select>
</mapper>