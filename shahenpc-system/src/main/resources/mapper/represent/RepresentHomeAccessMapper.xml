            <if test="latitude != null">latitude,</if>
            <if test="longitude != null">longitude,</if>
            <if test="address != null">address,</if>
            <if test="content != null">content,</if>
            <if test="picUrl != null">pic_url,</if>
            <if test="browseNumber != null">browse_number,</if>
            <if test="remark != null">remark,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="createBy != null">create_by,</if>
            <if test="mapWidth != null">map_width,</if>
            <if test="mapHeight != null">map_height,</if>
            <if test="mapArea != null">map_area,</if>
            <if test="location != null">location,</if>
            <if test="phone != null">phone,</if>
            <if test="leader != null">leader,</if>
            <if test="virtualMobile != null">virtual_mobile,</if>
            <if test="videoUserId != null">video_user_id,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="title != null">#{title},</if>
            <if test="provinceCode != null">#{provinceCode},</if>
            <if test="provinceName != null">#{provinceName},</if>
            <if test="cityCode != null">#{cityCode},</if>
            <if test="cityName != null">#{cityName},</if>
            <if test="areaCode != null">#{areaCode},</if>
            <if test="areaName != null">#{areaName},</if>
            <if test="latitude != null">#{latitude},</if>
            <if test="longitude != null">#{longitude},</if>
            <if test="address != null">#{address},</if>
            <if test="content != null">#{content},</if>
            <if test="picUrl != null">#{picUrl},</if>
            <if test="browseNumber != null">#{browseNumber},</if>
