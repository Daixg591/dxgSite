    <delete id="deleteRepresentHomeAccessByAccessId" parameterType="Long">
        delete from represent_home_access where access_id = #{accessId}
    </delete>

    <delete id="deleteRepresentHomeAccessByAccessIds" parameterType="String">
        delete from represent_home_access where access_id in
        <foreach item="accessId" collection="array" open="(" separator="," close=")">
            #{accessId}
        </foreach>
    </delete>
    <!--=========================================分割线-->
    <select id="selectVisitsCount" resultType="java.lang.Integer">
        SELECT COUNT(browse_number) FROM represent_home_access
    </select>

    <select id="rankingList" resultMap="RepresentHomeAccessResult">
        <include refid="selectRepresentHomeAccessVo"/> ORDER BY browse_number DESC LIMIT 3;
    </select>
