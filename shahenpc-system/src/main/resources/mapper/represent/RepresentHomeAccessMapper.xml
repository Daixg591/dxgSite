    <sql id="selectRepresentHomeAccessVo">
        select access_id, title, province_code, province_name, city_code, city_name, area_code, area_name, latitude, longitude, address, content, pic_url, browse_number, remark, create_time, update_by, update_time, create_by, map_width, map_height, map_area, location, phone, leader, virtual_mobile, video_user_id from represent_home_access
    </sql>

    <select id="selectRepresentHomeAccessList" parameterType="RepresentHomeAccess" resultMap="RepresentHomeAccessResult">
        <include refid="selectRepresentHomeAccessVo"/>
        <where>
            <if test="title != null  and title != ''"> and title = #{title}</if>
            <if test="provinceCode != null  and provinceCode != ''"> and province_code = #{provinceCode}</if>
            <if test="provinceName != null  and provinceName != ''"> and province_name like concat('%', #{provinceName}, '%')</if>
            <if test="cityCode != null  and cityCode != ''"> and city_code = #{cityCode}</if>
            <if test="cityName != null  and cityName != ''"> and city_name like concat('%', #{cityName}, '%')</if>
            <if test="areaCode != null  and areaCode != ''"> and area_code = #{areaCode}</if>
            <if test="areaName != null  and areaName != ''"> and area_name like concat('%', #{areaName}, '%')</if>
            <if test="latitude != null  and latitude != ''"> and latitude = #{latitude}</if>
            <if test="longitude != null  and longitude != ''"> and longitude = #{longitude}</if>
            <if test="address != null  and address != ''"> and address = #{address}</if>
            <if test="content != null  and content != ''"> and content = #{content}</if>
            <if test="picUrl != null  and picUrl != ''"> and pic_url = #{picUrl}</if>
            <if test="browseNumber != null "> and browse_number = #{browseNumber}</if>
            <if test="mapWidth != null "> and map_width = #{mapWidth}</if>
            <if test="mapHeight != null "> and map_height = #{mapHeight}</if>
            <if test="mapArea != null  and mapArea != ''"> and map_area = #{mapArea}</if>
            <if test="location != null  and location != ''"> and location = #{location}</if>
            <if test="phone != null  and phone != ''"> and phone = #{phone}</if>
            <if test="leader != null  and leader != ''"> and leader = #{leader}</if>
            <if test="virtualMobile != null  and virtualMobile != ''"> and virtual_mobile = #{virtualMobile}</if>
            <if test="videoUserId != null  and videoUserId != ''"> and video_user_id = #{videoUserId}</if>
        </where>
    </select>

    <select id="selectRepresentHomeAccessByAccessId" parameterType="Long" resultMap="RepresentHomeAccessResult">
        <include refid="selectRepresentHomeAccessVo"/>
        where access_id = #{accessId}
    </select>

    <insert id="insertRepresentHomeAccess" parameterType="RepresentHomeAccess" useGeneratedKeys="true" keyProperty="accessId">
