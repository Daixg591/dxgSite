<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shahenpc.system.mapper.personel.PersonnelAppointNoticeMapper">

    <resultMap type="PersonnelAppointNotice" id="PersonnelAppointNoticeResult">
        <result property="noticeId"    column="notice_id"    />
        <result property="coverUrl"    column="cover_url"    />
        <result property="title"    column="title"    />
        <result property="details"    column="details"    />
        <result property="state"    column="state"    />
        <result property="remark"    column="remark"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="delFlag"    column="del_flag"    />
        <result property="type"    column="type"    />
        <result property="columnType"    column="column_type"    />
        <result property="readCnt"    column="read_cnt"    />
        <result property="shareCnt"    column="share_cnt"    />
        <result property="banner"    column="banner"    />
        <result property="fileUrl"    column="file_url"    />
    </resultMap>

    <sql id="selectPersonnelAppointNoticeVo">
        select notice_id, cover_url, title, details, state, remark, create_by, create_time, update_by, update_time, del_flag, type, column_type, read_cnt, share_cnt, banner, file_url from personnel_appoint_notice
    </sql>

    <select id="selectPersonnelAppointNoticeList" parameterType="PersonnelAppointNotice" resultMap="PersonnelAppointNoticeResult">
        <include refid="selectPersonnelAppointNoticeVo"/>
        <where>
            <if test="coverUrl != null  and coverUrl != ''"> and cover_url = #{coverUrl}</if>
            <if test="title != null  and title != ''"> and title like concat('%', #{title}, '%')</if>
            <if test="details != null  and details != ''"> and details = #{details}</if>
            <if test="state != null  and state != ''"> and state = #{state}</if>
            <if test="type != null "> and type = #{type}</if>
            <if test="columnType != null "> and column_type = #{columnType}</if>
            <if test="readCnt != null "> and read_cnt = #{readCnt}</if>
            <if test="shareCnt != null "> and share_cnt = #{shareCnt}</if>
            <if test="banner != null "> and banner = #{banner}</if>
            <if test="fileUrl != null  and fileUrl != ''"> and file_url = #{fileUrl}</if>
        </where>
    </select>

    <select id="selectPersonnelAppointNoticeByNoticeId" parameterType="Long" resultMap="PersonnelAppointNoticeResult">
        <include refid="selectPersonnelAppointNoticeVo"/>
        where notice_id = #{noticeId}
    </select>

    <insert id="insertPersonnelAppointNotice" parameterType="PersonnelAppointNotice" useGeneratedKeys="true" keyProperty="noticeId">
        insert into personnel_appoint_notice
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="coverUrl != null">cover_url,</if>
            <if test="title != null">title,</if>
            <if test="details != null">details,</if>
            <if test="state != null">state,</if>
            <if test="remark != null">remark,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="delFlag != null">del_flag,</if>
            <if test="type != null">type,</if>
            <if test="columnType != null">column_type,</if>
            <if test="readCnt != null">read_cnt,</if>
            <if test="shareCnt != null">share_cnt,</if>
            <if test="banner != null">banner,</if>
            <if test="fileUrl != null">file_url,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="coverUrl != null">#{coverUrl},</if>
            <if test="title != null">#{title},</if>
            <if test="details != null">#{details},</if>
            <if test="state != null">#{state},</if>
            <if test="remark != null">#{remark},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="type != null">#{type},</if>
            <if test="columnType != null">#{columnType},</if>
            <if test="readCnt != null">#{readCnt},</if>
            <if test="shareCnt != null">#{shareCnt},</if>
            <if test="banner != null">#{banner},</if>
            <if test="fileUrl != null">#{fileUrl},</if>
        </trim>
    </insert>

    <update id="updatePersonnelAppointNotice" parameterType="PersonnelAppointNotice">
        update personnel_appoint_notice
        <trim prefix="SET" suffixOverrides=",">
            <if test="coverUrl != null">cover_url = #{coverUrl},</if>
            <if test="title != null">title = #{title},</if>
            <if test="details != null">details = #{details},</if>
            <if test="state != null">state = #{state},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
            <if test="type != null">type = #{type},</if>
            <if test="columnType != null">column_type = #{columnType},</if>
            <if test="readCnt != null">read_cnt = #{readCnt},</if>
            <if test="shareCnt != null">share_cnt = #{shareCnt},</if>
            <if test="banner != null">banner = #{banner},</if>
            <if test="fileUrl != null">file_url = #{fileUrl},</if>
        </trim>
        where notice_id = #{noticeId}
    </update>

    <delete id="deletePersonnelAppointNoticeByNoticeId" parameterType="Long">
        delete from personnel_appoint_notice where notice_id = #{noticeId}
    </delete>

    <delete id="deletePersonnelAppointNoticeByNoticeIds" parameterType="String">
        delete from personnel_appoint_notice where notice_id in
        <foreach item="noticeId" collection="array" open="(" separator="," close=")">
            #{noticeId}
        </foreach>
    </delete>
    <!--==============================自定义=========-->
</mapper>